{
    "manifest_version": 2,

    "name": "HackerNews No Pressure",
    "description": "Use HackerNews without scores",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["*://news.ycombinator.com/*"],
            "css": ["src/styles.css"],
            "run_at": "document_start"
        }
    ]
}
